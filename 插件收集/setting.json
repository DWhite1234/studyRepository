{
    "editor.fontFamily": "Consolas, 'Courier New', monospace",//字体
    "editor.fontSize": 20,//像素
    "editor.matchBrackets": true,//括号匹配
    "files.autoSave": "onFocusChange",//自动保存
    "editor.minimap.enabled": false,//缩略图
    "editor.maxTokenizationLineLength": 9999999,//单行最大字符长度
    "[vue]": {
        "editor.defaultFormatter": "octref.vetur"
    },
    "editor.hover.delay": 0,//悬停等待时间
    "editor.highlightActiveIndentGuide": false,//缩进参考线
    "editor.quickSuggestionsDelay": 0,//快速建议等待时间
    "workbench.sideBar.location": "left",//对齐方式
    "window.zoomLevel": 0,//窗口缩放比例，默认0
    "editor.suggest.snippetsPreventQuickSuggestions": false,//是否阻止快速建议
    "emmet.triggerExpansionOnTab": true,//tab快捷键
    "editor.mouseWheelZoom": true,
    "editor.quickSuggestions": true,//键入时显示建议
    "files.associations": {
        "*.vue": "vue",
        "*.js": "javascript"
    },
    "[html]": {
        "editor.suggest.insertMode": "replace"
    },
    "emmet.syntaxProfiles": {
        "vue-html": "html",
        "vue": "html"
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorTheme": "One Dark Pro"
}